<table class="pure-table pure-table-bordered">
  <tr>
    <th>Name</th>
    <th>Image</th>
  </tr>
  <% artists.forEach((x) => { %>
  <tr>
    <td><a href="<%= x.url %>"><%= x.name %></a></td>
    <% if (!x.img || !x.img.url) { %>
    <td>
      <!--
        https://placeholder.pics/
        https://placeholder.pics/svg/160/DEDEDE/555555/no%20image
      -->
      <svg width="160" height="160" xmlns="http://www.w3.org/2000/svg">
        <rect
          x="2"
          y="2"
          width="156"
          height="156"
          style="fill: #dedede; stroke: #555555; stroke-width: 2"
        />
        <text
          x="50%"
          y="50%"
          font-size="18"
          text-anchor="middle"
          alignment-baseline="middle"
          font-family="monospace, sans-serif"
          fill="#555555"
        >
          no image
        </text>
      </svg>
    </td>
    <% } else { %>
    <td><img crossorigin="anonymous" src="<%= x.img.url %>" /></td>
    <% } %>
  </tr>
  <% }) %>
</table>
